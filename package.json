{
    "name": "host-manager",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "npm-run-all -p lint:fix lint:script lint:style",
        "lint:fix": "prettier --loglevel=error -w src",
        "lint:script": "eslint --fix --ext .ts,tsx src",
        "lint:style": "stylelint --fix src/**/*.{css,less}",
        "prepare": "husky install"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=7"
    },
    "browserslist": {
        "development": [
            "last 1 chrome version"
        ],
        "production": [
            "> 1%",
            "last 2 versions",
            "not ie <= 11"
        ]
    },
    "dependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-router": "^6.3.0"
    },
    "devDependencies": {
        "@types/react": "^17.0.33",
        "@types/react-dom": "^17.0.10",
        "@typescript-eslint/eslint-plugin": "^5.19.0",
        "@vitejs/plugin-react": "^1.0.7",
        "eslint": "^8.13.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.0",
        "lint-staged": "^12.3.7",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.6.2",
        "stylelint": "^14.6.1",
        "typescript": "^4.5.4",
        "vite": "^2.9.0"
    }
}
