{
    "name": "host-manager",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview"
    },
    "dependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-router": "^6.3.0"
    },
    "devDependencies": {
        "@types/react": "^17.0.33",
        "@types/react-dom": "^17.0.10",
        "@vitejs/plugin-react": "^1.0.7",
        "eslint": "^8.13.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.3.7",
        "prettier": "^2.6.2",
        "typescript": "^4.5.4",
        "vite": "^2.9.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "eslint --cache --fix",
            "git add"
        ],
        "src/**/*.{js,jsx}": [
            "eslint --cache --fix",
            "git add"
        ]
    }
}
